{
    "name": "bupy7/yii2-flexslider",
    "description": "This is widget wrapper of Flexslider https://github.com/woothemes/FlexSlider for Yii2.",
    "keywords": ["yii2", "extension", "widget", "flexslider"],
    "homepage": "https://github.com/bupy7/yii2-flexslider",
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "authors": [
        {
            "name": "Vasilij Belosludcev",
            "email": "bupy765@gmail.com",
            "homepage": "http://mihaly4.ru"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "asset-repositories": {
            "bower-asset/FlexSlider": {
                "type": "bower-git",
                "url": "https://github.com/woothemes/FlexSlider.git"
            }
        }
    },
    "require": {
        "php": ">=5.4.0",
        "bower-asset/FlexSlider": "2.2.*"
    },
    "autoload": {
        "psr-4": {
            "bupy7\\flexslider\\": ""
        }
    }
}
